      "request": "launch",
      "name": "Web server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/koapp.js",
      "args": ["http", "-p", "8080"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Test web server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/tests/bootstrap.js",
      "args": []
